{
  "version": 2,
  "env": {
    "DATABASE_URL": "mongodb+srv://Karthi:karthi@cluster0.txoxi.mongodb.net/firstNest?retryWrites=true&w=majority"
  },
  "name": "first-nest",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    },
    {
      "src": "/auth",
      "methods": ["POST"],
      "dest": "src/main.ts",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
